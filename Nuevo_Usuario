CREATE USER nombre_de_usuario IDENTIFIED BY contrase√±a 
DEFAULT TABLESPACE "USERS"
TEMPORARY TABLESPACE "TEMP";

-- QUOTAS
ALTER USER Alumno QUOTA UNLIMITED ON USERS;

-- ROLES
